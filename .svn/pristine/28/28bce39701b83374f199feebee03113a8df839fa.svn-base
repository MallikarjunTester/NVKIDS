	@Override
	public void click(WebElement ele) 
	{
		try 
		{
			wait = new WebDriverWait(driver, 10);
			wait.until(ExpectedConditions.elementToBeClickable(ele));
			ele.click();
			log.info("The Button '" + ele.getText() + "' is clicked");
			
		
			if(ele.getAttribute("class").contentEquals("ui-menuitem-text"))
			{
			reportStep("The Link '" + ele.getText() + "' is clicked", "pass");
			}
			else if(ele.getAttribute("class").contains("chevron-down"))
			{
				reportStep("The Grid Accordion Icon is clicked", "pass");
			}
			else
			{
				
			reportStep("The Button '" + ele.getText() + "' is clicked", "pass");
			}
		} 
		catch (Exception e) 
		{
			int a = e.toString().indexOf("Exception:");
			String str = e.toString().substring(0, a + 10);
			reportStep("The Button '" + ele.getText() + "' not clicked because of - " + str, "fail");
			System.out.println(e.getMessage());
			log.error("The Button '" + ele.getText() + "' is not clicked");
		}
		finally 
		{
			takeSnap();
			log.info("Screenshot taken");
		}
	}

